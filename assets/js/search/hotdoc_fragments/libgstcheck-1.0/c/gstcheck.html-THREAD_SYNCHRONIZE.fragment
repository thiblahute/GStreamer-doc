fragment_downloaded_cb({"url": "libgstcheck-1.0/c/gstcheck.html#THREAD_SYNCHRONIZE", "fragment": "<div class=\"base_symbol_container\" data-hotdoc-tags=\"\" id=\"THREAD_SYNCHRONIZE\">\n\t\t<h3 id=\"thread_synchronize\">\n\t<span><code>THREAD_SYNCHRONIZE</code></span>\n</h3>\n\n\t\t<pre class=\"raw_code\">\n#define THREAD_SYNCHRONIZE()                                    \\\nG_STMT_START {                                                  \\\n  /* synchronize everyone */                                    \\\n  GST_DEBUG (\"THREAD %p: syncing\", g_thread_self ());           \\\n  fail_if (g_mutex_trylock (&amp;mutex),                            \\\n      \"bug in unit test, mutex should be locked at this point\");\\\n  g_cond_wait (&amp;sync_cond, &amp;mutex);                             \\\n  GST_DEBUG (\"THREAD %p: synced\", g_thread_self ());            \\\n  g_mutex_unlock (&amp;mutex);                                      \\\n} G_STMT_END;\n</pre>\n\n\n\n\n\n\n</div>\n\n\n\n"});